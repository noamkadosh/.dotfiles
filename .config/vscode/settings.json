	"css.validate": false,
	"editor.bracketPairColorization.enabled": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.cursorStyle": "block",
	"editor.cursorSurroundingLines": 100,
	"editor.cursorSurroundingLinesStyle": "default",
	"editor.fontFamily": "LeagueMono Narrow Nerd Font",
	"editor.fontLigatures": true,
	"editor.fontSize": 18,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": "active",
	"editor.inlineSuggest.enabled": true,
	"editor.lineHeight": 34,
	"editor.renderWhitespace": "all",
	"editor.rulers": [
		80
	],
	"editor.suggestSelection": "first",
	"editor.tabSize": 4,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"emmet.triggerExpansionOnTab": true,
	"eslint.packageManager": "pnpm",
	// "eslint.runtime": "node",
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"graphql"
	],
	// "files.autoSave": "afterDelay",
	// "files.autoSaveDelay": 3000,
	"git.confirmSync": false,
	"indentRainbow.colorOnWhiteSpaceOnly": true,
	"indentRainbow.colors": [
		"rgba(255, 0, 0, .0618)",
		"rgba(255, 166, 0, .0618)",
		"rgba(255, 255, 0, .0618)",
		"rgba(0, 128, 0, .0618)",
		"rgba(0, 0, 255, .0618)",
		"rgba(76, 0, 130, .0618)",
		"rgba(238, 130, 238, .0618)"
	],
	"indentRainbow.ignoreErrorLanguages": [
		"*"
	],
	"less.validate": false,
	// "liveServer.settings.CustomBrowser": "chrome",
	// "liveServer.settings.donotShowInfoMsg": true,
	"material-icon-theme.folders.associations": {
		".turbo": "Log",
		".vscode": "Vscode",
		"angular": "Angular",
		"benches": "Benchmark",
		"browserslist-config": "Config",
		"components": "React-components",
		"dom": "Base",
		"eslint-config": "Config",
		"migrations": "Generator",
		"postgres-data": "Database",
		"prettier-config": "Config",
		"react": "React-components",
		"react-ui": "React-components",
		"server-build": "Dist",
		"store": "Redux-store",
		"styles-compiled": "Css",
		"stylelint-config": "Config",
		"types": "TypeScript",
		"tsconfig": "TypeScript",
		"UI": "React-components",
		"workshop": "Storybook",
		"*-e2e": "e2e",
	},
	"material-icon-theme.files.associations": {
		"docker-compose.e2e.yml": "Docker",
		"docker-compose.dev.e2e.yml": "Docker",
		"*.tsconfig.json": "Tsconfig",
		"*.eslintrc.cjs": "ESLint",
		"*.eslintrc.js": "ESLint",
		"next.config.cjs": "Next",
		"remix.config.cjs": "Remix",
		"project.json": "Nrwl",
		"workspace.json": "Nrwl",
	},
	"prettier.arrowParens": "avoid",
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": false,
	"scss.validate": false,
	"stylelint.validate": [
		"css",
		"scss"
	],
	"terminal.external.osxExec": "Alacritty.app",
	"terminal.explorerKind": "external",
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.fontSize": 15,
	"turboConsoleLog.addSemicolonInTheEnd": true,
	"turboConsoleLog.quote": "`",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"window.title": "${rootName}${separator}${activeEditorShort}",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "none",
	"editor.stickyScroll.enabled": true,
	// "[postcss]": {
	// 	"editor.defaultFormatter": "esbenp.prettier-vscode"
	// },
	"workbench.colorTheme": "Tokyo Night",
	"github.copilot.enable": {
		"*": true,
		"yaml": true,
		"plaintext": false,
		"markdown": false,
		"rust": false
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"files.exclude": {
		"**/.git-worktrees": true
	},
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"Cargo.toml": "Cargo.lock",
		"*.ts": "${capture}.typegen.ts",
		"*.tsx": "${capture}.typegen.ts",
		"*.mts": "${capture}.typegen.ts",
		"*.cts": "${capture}.typegen.ts"
	},
	"workbench.productIconTheme": "fluent-icons",
	"[nix]": {
		"editor.defaultFormatter": "brettm12345.nixfmt-vscode"
	},
}